

<MudDialog>
    <TitleContent>
        @Title
    </TitleContent>
    <DialogContent>
        @Content


    </DialogContent>
</MudDialog>

@code {
    [CascadingParameter]
    private MudDialogInstance MudDialog { get; set; }

    [Parameter]
    public string Title { get; set; }
    [Parameter]
    public string Content { get; set; }
    [Parameter]
    public string Projects { get; set; }
}

